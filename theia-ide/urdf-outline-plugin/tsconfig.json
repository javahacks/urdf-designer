{
    "compilerOptions": {
        "skipLibCheck":true,
        "strict": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "es6",
        "lib": [
            "es2018",            
            "webworker"            
        ],
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "lib"
    },
    "include": [
        "src"
    ]
}
